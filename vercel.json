{
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "project_folder/WhenCanYouHangOut/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "project_folder/WhenCanYouHangOut/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
