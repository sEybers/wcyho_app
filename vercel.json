{
  "version": 2,
  "builds": [
    {
      "src": "project_folder/WhenCanYouHangOut/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/project_folder/WhenCanYouHangOut/dist/$1"
    }
  ],
  "functions": {
    "server/server.js": {
      "includeFiles": "server/**"
    }
  }
}
